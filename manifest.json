{
  "name": "Facefeedik",
  "version": "0.1.1",
  "description": "A Chrome extension that does some magic for the members of the very special Facebook group",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "init.js",
        "jquery-2.1.3.min.js",
        "actions/discover.js",
        "actions/show.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}
